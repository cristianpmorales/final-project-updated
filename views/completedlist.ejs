<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="adminstyle.css">
    <link rel="stylesheet" href="createList.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <title>Completed List</title>
  </head>
  <body>
    <section class="container">
        <header>
          <section class="page-header text-center">
              <h1>Completed List</h1>
          </section>
    <div class="containerNav">
      <nav class="primary_nav_wrap">
      <ul>
        <li> <a href="/create_campaign">Create Election Campaign</a></li>
        <li> <a href="/profile">Create a list</a></li>
        <li><a href="/">Go to Canvasser</a></li>
    </div>

    <section class="tableContainer">
          <h3>Canvasser List Below</h3>
          <table>
          <colgroup></colgroup>
          <thead>
            <tr>
              <th scope ="col">Adress</th>
              <th scope ="col">First Name</th>
              <th scope ="col">Last Name</th>
              <th scope ="col">Voting History</th>
              <th scope ="col">Delete</th>
            </tr>
          </thead>
          <tbody>
            <% peopleList.forEach( function ( peopleList ){ %>
              <% if (peopleList.votingActivity === 'Y') {%>
              <tr>
                <td data-label ="Adress">
                  <a class="" href="/editpeople/<%= peopleList._id %>"><%= peopleList.address %></a>
                </td>
                <td data-label ="First Name">
                  <a class="" href="/editpeople/<%= peopleList._id %>"><%= peopleList.firstName %></a>
                </td>
                <td data-label ="Last Name">
                  <a class="" href="/editpeople/<%= peopleList._id %>"><%= peopleList.lastName %></a>
                </td>
                <td data-label ="Voting History">
                  <a class="" href="/editpeople/<%= peopleList._id %>"><%= peopleList.votingActivity %></a>
                </td>
                <td data-label ="Adress">
                  <a class="" href="/destroypeople/<%= peopleList._id %>">Delete</a>
                </td>
              </td>
              </tr>
              <% }; %>
              <% }); %>
          </tbody>

        </table>
    </section>

  </body>
</html>
